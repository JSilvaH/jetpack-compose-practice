<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.silva.contact_rename">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />

    <application
        android:name=".ContactsRename"
        
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_contact_v2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_contact_v2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.app.Starting">
        <activity
            android:name=".chagebyhour.ChangeByHourActivity"
            android:exported="false"
            android:label="@string/title_activity_change_by_hour"
            android:theme="@style/Theme.Contactrename" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.app.Starting">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>